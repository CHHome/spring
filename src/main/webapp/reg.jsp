<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Insert title here</title>
</head>
<body>
	<fieldset>
	<legend>用户注册</legend>
	<form action="${pageContext.request.contextPath }/user/reg.do" method="post" enctype="multipart/form-data">
		<div>
			<label>用户名</label><input type="text" name="username" />
		</div>
		<div>
			<label>密码</label><input type="password" name="password" />
		</div>
		<div>
			<label>确认密码</label><input type="password" name="confirm" />
		</div>
		<div>
			<label>头像</label><input type="file" name="imgs" />
		</div>
		<div><button>提交</button> <input type="reset" value="重置" /></div>
	</form>
</fieldset>
</body>
</html>